<?xml version="1.0" encoding="utf-8" ?>
<base:ContentPageViewBase
    x:Class="NuSocial.Views.WalletView"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:base="clr-namespace:NuSocial.Core.View"
    xmlns:loc="clr-namespace:NuSocial.Helpers"
    xmlns:viewModels="clr-namespace:NuSocial.ViewModels"
    Title="{Binding Title}"
    x:DataType="viewModels:WalletViewModel"
    x:TypeArguments="viewModels:WalletViewModel">
    <VerticalStackLayout>
        <Label
            HorizontalOptions="Center"
            Text="Wallet"
            VerticalOptions="Center" />
    </VerticalStackLayout>
</base:ContentPageViewBase>